<?xml version="1.0" encoding="UTF-8"?>
<!-- This schema used to describe all messagea generated by DSMS CMD subsystem --> 

<xs:schema 
           targetNamespace="http://dsms.jpl.nasa.gov/cmd/schema" 
           xmlns="http://dsms.jpl.nasa.gov/cmd/schema"
           xmlns:dsms="http://dsms.jpl.nasa.gov/globalschema"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" 
           elementFormDefault="qualified">

    <xs:import namespace="http://dsms.jpl.nasa.gov/globalschema" 
               schemaLocation="/local/cmd/acmd/tables/schema/cmd/DSMSHeaderSchema.xsd"/>

    <!--CMD message for the DSMS Information Bus-->
    <xs:element name="CMDMessage">
        <xs:complexType>
            <xs:sequence>
                <xs:element xmlns:dsms="http://dsms.jpl.nasa.gov/globalschema" ref="dsms:DSMSHeader"/>
                <xs:element name="DSMSBody"   type="CMDMessageTypes"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:complexType name="CMDMessageTypes">
        <xs:all>
           <xs:element name="CMD_MESSAGE" type="CMDMessageBody"/>
        </xs:all>
    </xs:complexType>


    <xs:complexType name="CMDMessageBody">
        <xs:sequence>
            <xs:element name="MESSAGE_ID" type="xs:string" minOccurs="0"/>
            <!-- optional element -->
            <xs:element name="BC_DSS_ID"  type="xs:string" minOccurs="0"/>

            <xs:choice>
               <xs:element name="EVENT"        type="EventMessageType"/>
               <xs:element name="INSERT_SCMF"  type="UplinkRequestInfo"/>
               <xs:element name="STAGED_FILE"  type="StagedFileRequestInfo"/>

               <xs:element name="USER_LOG_RESPONSE" 
                           type="UserLogResponseType"/>

               <!-- response for all insert request -->
               <xs:element name="SPACECRAFT_CONFIG_RESPONSE"
                           type="SpacecraftConfigResponseType"/>
 
               <xs:element name="INSERT_RESPONSE"        type="InsertResponseType"/>
               <xs:element name="UPDATE_UPLINK_REQUEST"  type="UpdateUplinkRequestType"/>
               <xs:element name="UPDATE_RESPONSE"        type="InsertResponseType"/>
               <xs:element name="CREATE_CONNECTION"      type="MDCSessionRec"/>
               <xs:element name="CONNECTION_RESPONSE"    type="ConnectionCreationResponseType"/>
               <xs:element name="UPDATE_CONNECTION"      type="MDCUpdateSessionRec"/>
               <!-- response to Connection list/session query -->
               <xs:element name="CONNECTION_LIST"        type="ConnectionListResponseMessage"/>
               <!-- response to Connection list query -->
               <xs:element name="LIST_CONFIGURATION"     type="ListConfigurationResponseType"/>
               <!-- response to Role queue query -->
               <xs:element name="ROLE_POOL"             type="RolePoolResponseType"/>
               <!-- response to rad-list query -->
               <xs:element name="RADIATION_LIST"         type="RadListResponseMessageType"/>
               <!-- response to staged file add request -->
               <xs:element name="STAGED_FILE_ADD_RESPONSE" type="StagedListResponseMessageType"/>
               <!-- response to staged-list query -->
               <xs:element name="STAGED_LIST"            type="StagedListResponseMessageType"/>
               <!-- response to stored-list query -->
               <xs:element name="STORED_LIST"            type="StoredListResponseMessageType"/>
               <!-- response to autoget query -->
               <xs:element name="AUTOGET_LIST"           type="AutoGetResponseMessageType"/>
               <!-- response to scmf content query -->
               <xs:element name="SCMF_CONTENT"           type="ScmfFileContentType"/>
               <!-- response to autorad query -->
               <xs:element name="AUTORAD_LIST"           type="AutoradListResponseMessageType"/>
              <!-- response to query bitrate/cmg-mod/cmd-mod -->
               <xs:element name="PARAMETER_RESPONSE"     type="ParameterResponseMessageType"/>
               <!-- response to query about Production Status -->
               <xs:element name="PRODUCTION_STATUS_RESPONSE" type="ProductionStatusResponseMessageType"/>
               <!-- response to query about Connection Status -->
               <xs:element name="CONNECTION_STATUS_RESPONSE" type="ConnectionStatusResponseMessageType"/>
               <xs:element name="COMMAND_MNE_DATA"       type="MneDataResponseMessageType"/>
               <xs:element name="PRINT_RESPONSE"         type="PrintResponseMessageType"/>
               <!-- generic response -->
               <xs:element name="MDC_RESPONSE"           type="ResponseStatusType"/>
               <xs:element name="UPDATE_SERVER_CONF"     type="ServerConfUpdateMessageType"/>
               <xs:element name="SLE_AUTH_RESPONSE"      type="SleAuthResponseMessageType"/>

               <!-- +++++++++++++++++++ -->
               <!-- MDC Status messages -->
               <!-- +++++++++++++++++++ -->
               <xs:element name="UPLINK_REQ_ADDED"       type="InsertUplinkRequestType"/>
               <!-- currently only allow one update at a time, but just in case thing might
                    change in the future, we include a list here in the schema -->
               <xs:element name="UPLINK_REQ_UPDATED"     type="InsertUplinkRequestType"/>
               <xs:element name="UPLINK_REQ_DELETED"     type="RequestIdListTypeMin1"/>
               <xs:element name="CONNECTION_ADDED"       type="MDCSessionList"/>
               <xs:element name="CONNECTION_UPDATED"     type="MDCSessionList"/>
               <xs:element name="CONNECTION_DELETED"     type="SessionIdListType"/>
               <xs:element name="STAGED_FILE_ADDED"       type="StagedListMessageType"/>
               <xs:element name="STAGED_FILE_DELETED"     type="StagedListMessageType"/>
               <!-- connection status, look at "STATUS, only need to worry about 
                    CONNECTION_STATUS enum, ignore the rest -->
               <xs:element name="LIST_CONFIGURATION_INFO" type="ListConfiguration"/>
               <xs:element name="ROLE_POOL_UPDATED"       type="RolePoolList"/>
               <xs:element name="CLTU_RADIATED"           type="CltuRadiatedMessageType"/>
               <xs:element name="STATUS"                  type="StatusMessageType"/>
               <xs:element name="PARAMS"                  type="ParameterMessageType"/>
               <!-- above RADIATION_LIST can also be part of the MDC status messages -->
               <xs:element name="MONITOR_DATA"            type="MonitorDataType"/>
               <xs:element name="USER_PROFILE_RESPONSE"   type="UserProfileResponseMessageType"/>
               <!-- User access -->
               <xs:element name="USER_ACC_DELETED"        type="UserAccessDeltaMessageType"/>
               <xs:element name="USER_ACC_ADDED"          type="UserAccessDeltaMessageType"/>
               <xs:element name="USER_ACC_ALL"            type="UserAccessAllMessageType"/>
               <xs:element name="CLTU_RAD_RESPONSE"       type="CltuRadiatedResponseMessageType"/>

               <!-- System Configuration -->
               <xs:element name="SERVER_CONF"             type="ServerConfMessageType"/>

            </xs:choice>

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="UserLogResponseType">
        <xs:sequence>
            <xs:element name="RESPONSE" type="ResponseStatus" />
            <xs:element name="COMMON"   type="CommonData" 
                       minOccurs="1" maxOccurs="1"/>
            <xs:element name="LOG_LIST" type="LogList"
                       minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CommonData">
        <xs:sequence>
            <xs:element name="SC_ID" type="xs:string"
                                 minOccurs="1" maxOccurs="1"/>
            <xs:element name="USER_ID" type="String30"
                                 minOccurs="0" maxOccurs="1"/>            
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LogList">
        <xs:sequence>
            <xs:element name="LOG" type="LogInfo"
                                 minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LogInfo">
        <xs:all>
            <xs:element name="USER_ID" type="String30"
                                 minOccurs="0" maxOccurs="1"/>   

            <!-- required elements -->
            <xs:element name="REQ_TIME"  type="UTCDateTimeType"/>
            <xs:element name="REQ"       type="String512"/>

            <xs:element name="RESP_TIME" type="UTCDateTimeType"/>  
            <xs:element name="RESP"      type="String512"/>
       </xs:all>
    </xs:complexType>



    <xs:complexType name="SpacecraftConfigResponseType">
        <xs:sequence>
            <xs:element name="RESPONSE" type="ResponseStatus" />
            <xs:element name="COMMON"   type="CommonInfo" 
                       minOccurs="1" maxOccurs="1"/>
            <xs:element name="SPACECRAFT_INFO_LIST" type="SpacecraftInfoList"
                       minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CommonInfo">
        <xs:sequence>
            <xs:element name="GUI_CONNECTION" type="GuiConnection" minOccurs="1" maxOccurs="1"/>
            <xs:element name="DSS_INFO" type="DssInfo" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GuiConnection">
        <xs:sequence>
            <xs:element name="RESP_TIMEOUT" type="xs:integer" />
            <xs:element name="STATUS_RESP_TIMEOUT" type="xs:integer" />
            <xs:element name="REQ_FAIL_COUNT" type="xs:integer" />
            <xs:element name="RETRY_DELAY" type="xs:integer" />
        </xs:sequence>
    </xs:complexType>  

    <xs:complexType name="DssInfo">
        <xs:sequence>
            <xs:element name="DSS_ID" type="xs:string" 
                                 minOccurs="1" maxOccurs="unbounded"/>  
        </xs:sequence>
    </xs:complexType>  

    <xs:complexType name="SpacecraftInfoList">
        <xs:sequence>
            <xs:element name="SPACECRAFT_INFO" type="SpacecraftInfo"
                                 minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SpacecraftInfo">
        <xs:all>
            <xs:element name="SPACECRAFT_NAME" type="xs:string" 
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="SPACECRAFT_ID" type="xs:integer"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="MISSION_ID" type="xs:integer"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="RATE_INFO" type="RateRangeType"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="MOD_INDEX_INFO" type="ModIndexRangeType"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="ROLE_INFO_LIST" type="RoleInfoList"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="STAGE_FILE_GROUP" type="StageFileGroupType"
                                 minOccurs="1" maxOccurs="1"/>
                                 
            <xs:element name="STAGE_FILE_TYPE" type="StageFileType"
                                 minOccurs="1" maxOccurs="1"/>

            <xs:element name="GUI_SET_UP" type="GuiSetupType"
                                 minOccurs="1" maxOccurs="1"/>

       </xs:all>
    </xs:complexType>

    <xs:complexType name="RateRangeType">
        <xs:sequence>
	   <xs:element name="RATE" type="xs:float" 
                            minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="ModIndexRangeType">
        <xs:sequence>
	   <xs:element name="MOD_INDEX" type="xs:float" 
                            minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="RoleRangeType">
        <xs:sequence>
	   <xs:element name="ROLE_ID" type="AlphaNumeric64" 
                            minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="RoleInfoList">
        <xs:sequence>
	   <xs:element name="ROLE_INFO" type="RoleInfoType" 
                            minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="RoleInfoType">
        <xs:sequence>
	   <xs:element name="ROLE_ID" type="AlphaNumeric64" />
	   <xs:element name="VIEW_ONLY" type="TrueFalseStatusType" />
	   <xs:element name="EXCLUDABLE" type="TrueFalseStatusType" />
	   <xs:element name="PRIORITIES" type="xs:string" />
           <xs:element name="MAX_ALLOWED" type="xs:integer" />
	   <xs:element name="ACTION" type="RoleActionType"/>
	</xs:sequence>
    </xs:complexType>


    <xs:complexType name="StageFileGroupType">
        <xs:sequence>
	   <xs:element name="GROUP" type="xs:string" 
                            minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="StageFileType">
        <xs:sequence>
	   <xs:element name="TYPE" type="xs:string" 
                            minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="GuiSetupType">
        <xs:sequence>
           <xs:element name="TEST_MODE" type="TrueFalseStatusType"/>
           <xs:element name="INCLUDE_MOD_INDEX" type="TrueFalseStatusType"/>
           <xs:element name="AUTORAD_ENABLED" type="TrueFalseStatusType"/>
           <xs:element name="DISP_SESSION_TAB" type="TrueFalseStatusType"/>
           <xs:element name="DISP_LAST_RAD_COL" type="TrueFalseStatusType"/>
           <xs:element name="DISP_BIT_RATE_SEL" type="TrueFalseStatusType"/>
           <xs:element name="DISP_BIT1_RQST" type="TrueFalseStatusType"/>
           <xs:element name="DISP_OPEN_CLOSE" type="TrueFalseStatusType"/>
           <xs:element name="DISP_CMG_MOD" type="TrueFalseStatusType"/>
           <xs:element name="DISP_CMG_MOD_CNTL" type="TrueFalseStatusType"/>
           <xs:element name="DISP_RANGE_ENABLE" type="TrueFalseStatusType"/>
           <xs:element name="DISP_RANGE_ENABLE_CNTL" type="TrueFalseStatusType"/>
           <xs:element name="DISP_DELAY" type="TrueFalseStatusType"/>
           <xs:element name="DISP_MNE_EDITOR" type="TrueFalseStatusType"/>
           <xs:element name="FILL_IN_CHECKSUM" type="TrueFalseStatusType"/>
           <xs:element name="PROMPT_USER_FOR_RAD_SCMF" type="TrueFalseStatusType"/>
           <xs:element name="PROMPT_USER_FOR_RAD_SCMF_WHEN_IMPORT" type="TrueFalseStatusType"/>
           <xs:element name="APPROVE_REQUEST" type="TrueFalseStatusType"/>
           <xs:element name="DISP_RQST_APPROVAL" type="TrueFalseStatusType"/>
           <xs:element name="DISP_UPLINK_RQST_PRIORITY" type="TrueFalseStatusType"/>
           <xs:element name="MAX_FILES_DISPLAYED" type="xs:integer"/>
           <xs:element name="MAX_DOM_FILE_LIST_SIZE" type="xs:integer"/>
           <xs:element name="NUMBER_OF_DAYS_TO_RETRIEVE" type="xs:integer"/>
           <xs:element name="DOM_IN_USED" type="TrueFalseStatusType"/>
           <xs:element name="CLTUS_RETAINED_PERIOD" type="xs:integer"/>
           <xs:element name="CLTUS_SAVED" type="xs:integer"/>
           <xs:element name="CLTUS_DISPLAYED" type="xs:integer"/>
           <xs:element name="CONNECTION_TIME_DELTA" type="xs:integer"/>
           <xs:element name="DISCONNECTION_TIME_DELTA" type="xs:integer"/>
           <xs:element name="START_RADIATION_TIME_DELTA" type="xs:integer"/>
           <xs:element name="END_RADIATION_TIME_DELTA" type="xs:integer"/>
           <xs:element name="OPEN_WIN_TIME_DELTA" type="xs:integer"/>
           <xs:element name="CLOSE_WIN_TIME_DELTA" type="xs:integer"/>
           <xs:element name="BIT1_TIME_DELTA" type="xs:integer"/>
           <xs:element name="DEFAULT_UPLINK_PRIORITY" type="xs:integer"/>
           <xs:element name="DEFAULT_AUTORAD_UPLINK_PRIORITY" type="xs:integer"/>
           <xs:element name="INACTIVE_TIMEOUT_PERIOD" type="xs:integer"/>
           <xs:element name="USER_RESPONSE_TIMEOUT_PERIOD" type="xs:integer"/>
           <xs:element name="DFS_FILES_PER_PAGE" type="xs:integer"/>
           <xs:element name="DFS_FF_PAGE_COUNT" type="xs:integer"/>
           <xs:element name="DISP_DB_PURGE_PROMPT" type="TrueFalseStatusType"/>
           <xs:element name="PURGE_DB_DEFAULT_ACTION" type="TrueFalseStatusType"/>
           <xs:element name="DISP_XLT_SESSION_SELECTION" type="TrueFalseStatusType"/>
           <xs:element name="CAUTIONARY_PROMPT" type="TrueFalseStatusType"/>
           <xs:element name="DISP_ROLE_EXCLUSION" type="TrueFalseStatusType"/>
           <xs:element name="REFRESH_DSN_FILELIST" type="TrueFalseStatusType"/>
           <xs:element name="SOURCE_ID" type="xs:string" minOccurs="0"/>
	</xs:sequence>
    </xs:complexType>

    <xs:simpleType name="RequestResultStatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="OK"/>
            <xs:enumeration value="ERROR"/>
       </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UrgencyType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="IMMEDIATELY"/>
            <xs:enumeration value="AFTER_FILE"/>
            <xs:enumeration value="AFTER_SESSION"/>
       </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="OpenCloseWindowList">
        <xs:sequence>
	   <xs:element name="OPEN_CLOSE_WINDOW" type="AROpenCloseWindow" 
                       minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:complexType name="AROpenCloseWindow">
        <xs:all>
            <xs:element name="OPEN_WINDOW_TIME"  type="UTCDateTimeType"
                minOccurs="1" maxOccurs="1"/>
            <xs:element name="CLOSE_WINDOW_TIME" type="UTCDateTimeType"
                minOccurs="1" maxOccurs="1"/>
            <xs:element name="USER_ID"           type="xs:string"
                minOccurs="0"/>
            <xs:element name="MODIFICATION_TIME" type="UTCDateTimeType"
                minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="SessionIdList">
        <xs:sequence>
	   <xs:element name="SESSION_ID" type="xs:string" 
                       minOccurs="1" maxOccurs="unbounded"/>
	</xs:sequence>
    </xs:complexType>

    <xs:simpleType name="SuspendType"> 
        <xs:restriction base="xs:string">
            <xs:enumeration value="SUSPENDING"/>
            <xs:enumeration value="SUSPENDED"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="BitRateModIndex">
        <xs:all>
            <xs:element name="BIT_RATE"  type="BitRateRange"
                        minOccurs="1" maxOccurs="1"/>
            <xs:element name="MOD_INDEX" type="ModIndexRange"
                        minOccurs="1" maxOccurs="1"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="CltuRadiatedMessageType">
        <xs:all>
            
            <!-- required elements -->
            <xs:element name="MESSAGE_NUMBER_WITHIN_REQUEST" type="xs:positiveInteger"/>
            <xs:element name="REQUEST_ID"                 type="xs:string"/>
            <xs:element name="MESSAGE_NUMBER_AT_STATION"  type="xs:nonNegativeInteger"/>

            <!-- optional elements -->
            <xs:element name="FILE_NAME"  type="xs:string" minOccurs="0"/>
            <xs:element name="START_TIME" type="UTCDateTimeType" minOccurs="0"/>
            <xs:element name="END_TIME"   type="UTCDateTimeType" minOccurs="0"/>
            <xs:element name="SEC_TIME"   type="UTCDateTimeType" minOccurs="0"/>
            <xs:element name="ER_TIME"    type="UTCDateTimeType" minOccurs="0"/>
            <xs:element name="REMAINING_RADIATION_TIME" type="DurationType" minOccurs="0"/>
            <xs:element name="COMMENT"    type="xs:string"           minOccurs="0"/>

        </xs:all>
    </xs:complexType>

    <xs:simpleType name="MdcConnectionStatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CONNECTED"/>
            <xs:enumeration value="DISCONNECTED"/>
            <xs:enumeration value="PENDING"/>
            <xs:enumeration value="TERMINATING"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EventCatagoryType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ADVISORY"/>
            <xs:enumeration value="ALARM"/>
            <xs:enumeration value="WARNING"/>
            <xs:enumeration value="LOG"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EventType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CONN"/>
            <xs:enumeration value="RADN"/>
            <xs:enumeration value="PMD"/>
            <xs:enumeration value="CFDP"/>
            <xs:enumeration value="TX"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="OnOffStatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ON"/>
            <xs:enumeration value="OFF"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ProductionStatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CONFIGURED"/>
            <xs:enumeration value="OPERATIONAL"/>
            <xs:enumeration value="HALTED"/>
            <xs:enumeration value="INTERRUPTED"/>
        </xs:restriction>
    </xs:simpleType>

   <!--Enumerated explaining the previous radiation status -->
    <xs:simpleType name="PreviousRadiationResultType">
        <xs:restriction base="xs:string">
            <!-- SCMF status -->
            <xs:enumeration value="RADIATED"/>
            <xs:enumeration value="FAILED_TO_RADIATE"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TrueFalseStatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="TRUE"/>
            <xs:enumeration value="true"/>
            <xs:enumeration value="FALSE"/>
            <xs:enumeration value="false"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RoleActionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="IGNORE"/>
            <xs:enumeration value="WARNING"/>
            <xs:enumeration value="PROHIBIT"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="YesType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="YES"/>
        </xs:restriction>
    </xs:simpleType>

    <!--Type for the UTC SFOC Format: YYYY-DDDTHH.MM:SS.sss-->
    <xs:simpleType name="UTCDateTimeType">
        <xs:annotation>
        <xs:documentation> </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!--could be YYYY-DOYTHH:MM:SS.sss OR -->
            <!--could be YYYY-DOYTHH:MM:SS     OR -->
            <!--                  HH:MM:SS.sss OR -->
            <!--         IGNORE                OR -->
            <!--         *                        -->
            <xs:pattern value="[0-9]{4}\-[0-3][0-9]{2}T[0-2][0-9]:[0-5][0-9]:[0-5][0-9]\.[0-9]{3}|[0-9]{4}\-[0-3][0-9]{2}T[0-2][0-9]:[0-5][0-9]:[0-5][0-9]|[0-2][0-9]:[0-5][0-9]:[0-5][0-9]\.[0-9]{3}|IGNORE"/>
        </xs:restriction>
    </xs:simpleType>

    <!--Type for the Duration time format: [H]HH.MM:SS.sss -->
    <xs:simpleType name="DurationType">
        <xs:annotation>
        <xs:documentation> </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{3}:[0-5][0-9]:[0-5][0-9]\.[0-9]{3}|[0-9]{2}:[0-5][0-9]:[0-5][0-9]\.[0-9]{3}"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- with blank-->
    <xs:simpleType name="AlphaNumeric32">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="32"/>
            <xs:pattern value="([a-zA-Z0-9_\.\-])*"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="AlphaNumeric64">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="64"/>
            <xs:pattern value="([a-zA-Z0-9_\.\-])*"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="AlphaNumeric86">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="86"/>
            <xs:pattern value="([a-zA-Z0-9_\.\-])*"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="AlphaNumeric100">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
            <xs:pattern value="([a-zA-Z0-9_\.\-])*"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="AlphaNumeric200">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="200"/>
            <xs:pattern value="([a-zA-Z0-9_\.\-])*"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="String30">
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="30"/>
        </xs:restriction>        
    </xs:simpleType>

    <xs:simpleType name="String200">
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="200"/>
